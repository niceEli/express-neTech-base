{
  "name": "express-netech-base",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --config .config/nodemon.json",
    "deno:dev": "nodemon --config .config/nodemon.deno.json",
    "bun:dev": "nodemon --config .config/nodemon.bun.json",
    "test": "vitest run",
    "test:watch": "vitest .",
    "format": "prettier --write .",
    "build": "tsx ./scripts/build/esbuild_build.mts",
    "deno:build": "deno -A ./scripts/build/esbuild_build.mts",
    "bun:build": "bun run bun:build:noCompile && bun build ./dist/index.js --compile --outfile=dist/build",
    "bun:build:noCompile": "bun run ./scripts/build/esbuild_build.mts",
    "start": "node --enable-source-maps ./dist/index.js",
    "deno:start": "deno --unstable-detect-cjs -A ./dist/index.js",
    "bun:start": "bun run ./dist/index.js",
    "dockerBuild": "docker build -t express-netech-base .",
    "postinstall": "npm run build"
  },
  "bin": "./dist/index.js",
  "keywords": [],
  "author": "niceEli",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "express": "^4.21.1",
    "inquirer": "^9.3.7",
    "tslog": "^4.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^5.0.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.16.13",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "prettier": "3.3.2",
    "ts-loader": "^9.5.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^7.18.0",
    "vitest": "^1.6.0"
  }
}
